<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frank.lotterdemo.mapper.AwardBatchMapper">

    <resultMap id="BaseResultMap" type="com.frank.lotterdemo.domain.AwardBatch">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="amount" column="amount" jdbcType="INTEGER"/>
            <result property="balance" column="balance" jdbcType="INTEGER"/>
            <result property="lastUpdateTime" column="last_update_time" jdbcType="TIME"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,amount,
        balance,last_update_time
    </sql>
</mapper>
